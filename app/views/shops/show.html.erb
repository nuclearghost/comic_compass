<%- model_class = Shop -%>
<div class="page-header">
  <h1><%=t '.title', :default => Shop %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @shop.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address) %></dt>
      <dd><pre class="prettyprint"><%= @shop.address %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address2) %></dt>
      <dd><pre class="prettyprint"><%= @shop.address2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @shop.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:state) %></dt>
      <dd><pre class="prettyprint"><%= @shop.state %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:scf) %></dt>
      <dd><pre class="prettyprint"><%= @shop.scf %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zip) %></dt>
      <dd><pre class="prettyprint"><%= @shop.zip %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:mailscore) %></dt>
      <dd><pre class="prettyprint"><%= @shop.mailscore %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic1code) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic1code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic1) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic2code) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic2code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic2) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic3code) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic3code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic3) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic3 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic4code) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic4code %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sic4) %></dt>
      <dd><pre class="prettyprint"><%= @shop.sic4 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:latitude) %></dt>
      <dd><pre class="prettyprint"><%= @shop.latitude %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:longitude) %></dt>
      <dd><pre class="prettyprint"><%= @shop.longitude %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:foursquare_id) %></dt>
      <dd><pre class="prettyprint"><%= @shop.foursquare_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email) %></dt>
      <dd><pre class="prettyprint"><%= @shop.email %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone) %></dt>
      <dd><pre class="prettyprint"><%= @shop.phone %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              shops_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_shop_path(@shop), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              shop_path(@shop),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

